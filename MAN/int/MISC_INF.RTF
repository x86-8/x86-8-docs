\par   {\cf2 2} {\cf6 2} > Data Bit 0      {\cf6 20} {\cf2 20}         {\cf2 11} {\cf6 11} < Busy           {\cf6 24} {\cf2 29}
\par   {\cf2 3} {\cf6 3} >  "    "  1      {\cf6 20} {\cf2 21}         {\cf2 12} {\cf6 12} < Paper End      {\cf6 25} {\cf2 30}
\par   {\cf2 4} {\cf6 4} >  "    "  2      {\cf6 21} {\cf2 22}         {\cf2 13} {\cf6 13} < Select         {\cf6 18} {\cf2 16,33}
\par   {\cf2 5} {\cf6 5} >  "    "  3      {\cf6 21} {\cf2 23}         {\cf2 14} {\cf6 14} > ^Auto Feed xt  {\cf6 18} {\cf2 16,33}
\par   {\cf2 6} {\cf6 6} >  "    "  4      {\cf6 22} {\cf2 24}         {\cf2 32} {\cf6 15} < ^Error         {\cf6 18} {\cf2 16,33}
\par   {\cf2 7} {\cf6 7} >  "    "  5      {\cf6 22} {\cf2 25}         {\cf2 31} {\cf6 16} > ^Initialize    {\cf6 18} {\cf2 16,33}
\par   {\cf2 8} {\cf6 8} >  "    "  6      {\cf6 23} {\cf2 26}         {\cf2 36} {\cf6 17} > ^Select In     {\cf6 18} {\cf2 16,33}
